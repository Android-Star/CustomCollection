<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="WaveProgressView">
    <attr name="wave_color"
        format="color"></attr>
    <attr name="bg_color"
        format="color"></attr>
    <attr name="second_wave_color"
        format="color"></attr>

    <attr name="wave_width"
        format="dimension"></attr>
    <attr name="wave_height"
        format="dimension"></attr>
  </declare-styleable>

  <declare-styleable name="CircleProgressView">
    <attr name="arcColor"
        format="color"></attr>
    <attr name="bgColor"
        format="color"></attr>
    <attr name="textColor"
        format="color"></attr>
    <attr name="radius"
        format="dimension"></attr>
    <attr name="arcWidth"
        format="dimension"></attr>
    <attr name="textSize"
        format="dimension"></attr>
  </declare-styleable>

  <declare-styleable name="GradientCircleProgressView">
    <attr name="arcColor1"
        format="color"></attr>
    <attr name="arcBgColor"
        format="color"></attr>
    <attr name="textColor1"
        format="color"></attr>
    <attr name="radius1"
        format="dimension"></attr>
    <attr name="arcWidth1"
        format="dimension"></attr>
    <attr name="textSize1"
        format="dimension"></attr>
    <attr name="startColor"
        format="color">
    </attr>
    <attr name="endColor"
        format="color">
    </attr>
  </declare-styleable>

  <declare-styleable name="RadarScanView">
    <attr name="radarBgColor"
        format="color"></attr>
    <attr name="defaultColor"
        format="color"></attr>
    <attr name="pointColor"
        format="color"></attr>
    <attr name="gradientStartColor"
        format="color"></attr>
    <attr name="gradientStopColor"
        format="color"></attr>
    <attr name="lineWidth"
        format="dimension"/>
    <attr name="pointNum"
        format="integer"></attr>
  </declare-styleable>

  <declare-styleable name="ChengyuSeekBar">
    <attr name="normalBgColor"
        format="color"></attr>
    <attr name="completeBgColor"
        format="color"></attr>
    <attr name="normalTextColor"
        format="color"></attr>
    <attr name="completeTextColor"
        format="color"></attr>

  </declare-styleable>

  <declare-styleable name="RecordButtonView">
    <attr name="bigCircleColor"
        format="color"/>
    <attr name="smallCircleColor"
        format="color"/>
    <attr name="smallRadius"
        format="dimension"/>
    <attr name="imgId"
        format="reference"/>
    <attr name="startAlpha"
        format="float"/>
    <attr name="endAlpha"
        format="float"/>
  </declare-styleable>

  <declare-styleable name="QQBezierView">
    <attr name="bubbleTxtColor" format="color"/>
    <attr name="bubbleColor" format="color"/>
    <attr name="bubbleTxtSize" format="dimension"/>
    <attr name="bubbleRadius" format="dimension"/>
    <attr name="bubbleTxt" format="string|reference"/>
  </declare-styleable>
</resources>