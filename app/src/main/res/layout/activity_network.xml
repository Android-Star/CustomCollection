<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
  <Button
      app:layout_constraintVertical_chainStyle="packed"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintBottom_toTopOf="@id/btnRetrofit"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:id="@+id/btnOkhttp"
      tools:ignore="MissingConstraints"
      app:layout_constraintLeft_toLeftOf="parent"
      app:layout_constraintRight_toRightOf="parent"
      android:text="okHttp"
      />
  <Button
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:id="@+id/btnRetrofit"
      tools:ignore="MissingConstraints"
      app:layout_constraintTop_toBottomOf="@id/btnOkhttp"
      app:layout_constraintBottom_toTopOf="@id/btnVolleyString"
      app:layout_constraintLeft_toLeftOf="parent"
      app:layout_constraintRight_toRightOf="parent"
      android:text="retrofit"
      />
  <Button
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:id="@+id/btnVolleyString"
      tools:ignore="MissingConstraints"
      app:layout_constraintTop_toBottomOf="@id/btnRetrofit"
      app:layout_constraintBottom_toTopOf="@id/btnVolleyJson"
      app:layout_constraintLeft_toLeftOf="parent"
      app:layout_constraintRight_toRightOf="parent"
      android:text="volley string"
      />
  <Button
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:id="@+id/btnVolleyJson"
      tools:ignore="MissingConstraints"
      app:layout_constraintTop_toBottomOf="@id/btnVolleyString"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintLeft_toLeftOf="parent"
      app:layout_constraintRight_toRightOf="parent"
      android:text="volley json"
      />
</android.support.constraint.ConstraintLayout>